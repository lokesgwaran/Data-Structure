#include <stdio.h>
#define SIZE 20

int visited[SIZE];

void dfs(int adj[SIZE][SIZE], int n, int v) {
    int i;
    visited[v] = 1;
    printf("%d ", v);
    for(i = 0; i < n; i++) {
        if(adj[v][i] && !visited[i])
            dfs(adj, n, i);
    }
}

int main() {
    int n, i, j, adj[SIZE][SIZE], start;
    printf("Enter number of vertices: ");
    scanf("%d", &n);
    printf("Enter adjacency matrix:\n");
    for(i = 0; i < n; i++)
        for(j = 0; j < n; j++)
            scanf("%d", &adj[i][j]);
    for(i = 0; i < n; i++) visited[i] = 0;
    printf("Enter starting vertex: ");
    scanf("%d", &start);
    printf("DFS Traversal: ");
    dfs(adj, n, start);
